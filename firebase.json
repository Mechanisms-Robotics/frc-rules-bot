{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "yarn --cwd \"$RESOURCE_DIR\" install --ignore-engines",
        "\"$RESOURCE_DIR\"/node_modules/.bin/tsc --project \"$RESOURCE_DIR\"/tsconfig.json"
      ]
    }
  ]
}
